{
	"name": "@gramio/types",
	"version": "7.11.0",
	"homepage": "https://github.com/gramiojs/types",
	"readme": "https://github.com/gramiojs/types",
	"description": "Code-generated and Auto-published Telegram Bot API types",
	"type": "commonjs",
	"author": "kravets",
	"license": "MIT",
	"keywords": [
		"telegram",
		"bot",
		"tba",
		"types",
		"telegram-bot-api",
		"code-generated",
		"auto-published",
		"cicd",
		"api"
	],
	"types": "./out/index.d.ts",
