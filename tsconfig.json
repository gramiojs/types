{
    "compilerOptions": {
        "target": "es2022",
        "module": "ES2022",
        "noEmit": true,
        "declaration": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true,
        "strict": true,
        "moduleResolution": "node",
        "strictNullChecks": true,
    },
}
